{
    "expo": {
        "name": "Misaina",
        "description": "Misaina is simple memory game for developer. Feel free to check source code at https://github.com/riv0manana/Misaina.",
        "slug": "misaina",
        "privacy": "public",
        "sdkVersion": "35.0.0",
        "platforms": [
            "ios",
            "android",
            "web"
        ],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "package": "mg.riv0manana.misaina"
        }
    }
}